# ===========================================
# Environment: Development (AWS Resources)
# ===========================================

# Application
NODE_ENV=development
NEXT_PUBLIC_APP_ENV=dev

# API Configuration
NEXT_PUBLIC_API_URL=/api

# AWS Configuration (Development)
NEXT_PUBLIC_AWS_REGION=eu-west-1
NEXT_PUBLIC_AWS_USER_POOL_ID=eu-west-1_DevPoolId
NEXT_PUBLIC_AWS_USER_POOL_CLIENT_ID=dev-cognito-client-id
NEXT_PUBLIC_AWS_APPSYNC_ENDPOINT=https://dev-xxxxxxxxxx.appsync-api.eu-west-1.amazonaws.com/graphql

# DynamoDB (AWS Development)
DYNAMODB_TABLE_PREFIX=dev_

# Debug & Logging
NEXT_PUBLIC_DEBUG=true
LOG_LEVEL=debug

# Google Drive Integration (Development)
NEXT_PUBLIC_GOOGLE_DRIVE_ENABLED=false
